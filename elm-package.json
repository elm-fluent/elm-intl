{
    "version": "2.0.0",
    "summary": "Bindings to JavaScript Internationalization API",
    "repository": "https://github.com/vanwagonet/elm-intl.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Intl.Collator",
        "Intl.Currency",
        "Intl.DateTimeFormat",
        "Intl.Locale",
        "Intl.NumberFormat",
        "Intl.PluralRules",
        "Intl.TimeZone"
    ],
    "native-modules": true,
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
